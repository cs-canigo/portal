{
    "config": {
        "auto.register.schemas": "false",
        "connector.class": "io.debezium.connector.oracle.OracleConnector",
        "consumer.override.security.protocol": "SSL",
        "consumer.override.ssl.key.password": "<jks-password>",
        "consumer.override.ssl.keystore.location": "/opt/confluent/certs/<client-name>.keystore.jks",
        "consumer.override.ssl.keystore.password": "<jks-password>",
        "consumer.override.ssl.truststore.location": "/opt/confluent/certs/<client-name>.keystore.jks",
        "consumer.override.ssl.truststore.password": "<jks-password>",
        "database.connection.adapter": "logminer",
        "database.dbname": "<dbname>",
        "database.password": "<password>",
        "database.port": "<port>",
        "database.server.id": "<server-id>",
        "database.server.name": "<server-name>",
        "database.url": "<connection-url>",
        "database.user": "<user>",
        "decimal.handling.mode": "string",
        "event.processing.failure.handling.mode": "fail",
        "heartbeat.interval.ms": "60000",
        "interval.handling.mode": "numeric",
        "key.converter": "<converter>",
        "key.converter.schemas.enable": "false",
        "log.mining.strategy": "online_catalog",
        "producer.override.security.protocol": "SSL",
        "producer.override.ssl.key.password": "<jks-password>",
        "producer.override.ssl.keystore.location": "/opt/confluent/certs/<client-name>.keystore.jks",
        "producer.override.ssl.keystore.password": "<jks-password>",
        "producer.override.ssl.truststore.location": "/opt/confluent/certs/<client-name>.keystore.jks",
        "producer.override.ssl.truststore.password": "<jks-password>",
        "provide.transaction.metadata": "true",
        "snapshot.mode": "no_data",
        "table.include.list": "<table_list>",
        "tasks.max": "1",
        "tombstones.on.delete": "false",
        "topic.heartbeat.prefix": "<client-name>-<description>-heartbeat",
        "topic.prefix": "<client-name>-inico-ks-sincro",
        "topic.transaction": "<client-name>-<description>-transactions",
        "use.latest.version": "true",
        "value.converter": "<converter>",
        "value.converter.schemas.enable": "false"
    },
    "name": "<client-name>-<description>"
}
